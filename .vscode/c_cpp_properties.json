{
    "enableConfigurationSquiggles": true,
    "env": {
        "Project_Include": [
            "${workspaceFolder}/**"
        ],
        "Project_Browse": [
            "${workspaceFolder}/"
        ],
        "Project_Forced_Include": []
    },
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${Project_Browse}"
                ]
            },
            "forcedInclude": [
                "${Project_Forced_Include}"
            ],
            "includePath": [
                "${Project_Include}"
            ],
            "defines": [
            ],
            "intelliSenseMode": "msvc-x64",
            "compilerArgs": [
                "-Wno-unknown-pragmas",
                "-Wno-attributes"
            ],
            
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}