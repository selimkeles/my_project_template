{
    "enableConfigurationSquiggles": true,
    "env": {
        "Project_Include": [
            "${workspaceFolder}/**"
        ],
        "Project_Browse": [
            "${workspaceFolder}/"
        ],
        "Project_Forced_Include": []
    },
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${Project_Browse}"
                ]
            },
            "forcedInclude": [
                "${Project_Forced_Include}"
            ],
            "includePath": [
                "${Project_Include}"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "configurationProvider": "ms-vscode.cmake-tools"        // This line directs everything to CMakeLists.txt so above is rubbish
        }
    ],
    "version": 4
}